{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
      "react": ["node_modules/@types/react"],
      "@types/react": ["node_modules/@types/react"]
    }
  },
  "include": ["mock/**/*", "src/**/*", "typings.d.ts", "src"],
  "exclude": ["node_modules", "lib", "es", "dist", "typings", "docs"]
}
